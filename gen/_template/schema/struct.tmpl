{{- /*gotype: github.com/ogen-go/ogen/internal/ir.Type*/ -}}
{{- define "schema/struct" }}
{{- if eq (len $.Fields) 0 }}{{/* Blank struct */}}
	type {{ $.Name }} struct{}
{{- else }}
	type {{ $.Name }} struct {
	{{- range $f := $.Fields }}
		{{- template "godoc" $f.GoDoc }}
		{{ $f.Name }} {{ $f.Type.Go }} {{ if $f.Tag.JSON }}{{ printf "json:%q" $f.Tag.JSON | quote }}{{ end }}
	{{- end }}
	}
{{- end }}

{{ end }}
